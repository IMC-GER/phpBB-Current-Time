	"version": "0.14.0",
	"time": "2025-06-05",
	"license": "GPL-2.0-only",
	"authors": [
        {
			"name": "Thorsten Ahlers",
			"homepage": "https://github.com/IMC-GER",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=7.4"
	},
	"extra": {
		"display-name": "Current Time / World Clock",
		"soft-require": {
			"phpbb/phpbb": ">=3.3.0,<4.0.0@dev"
		}
	}
}
